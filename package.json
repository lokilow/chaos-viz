{
  "name": "chaos",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "dev": "bun run build:wasm && vite",
    "build": "bun run build:wasm && tsc && vite build",
    "build:wasm": "cd core && wasm-pack build --target web --out-dir ../src/pkg",
    "uiua:check": "uiua check uiua-modules",
    "uiua:clean-cache": "uiua module clean",
    "uiua:eval": "bash scripts/uiua-wasm-eval.sh",
    "uiua:sync-defs": "bash scripts/sync-uiua-primitive-defs.sh",
    "format": "prettier . --write",
    "prepare": "husky"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  },
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^22.19.7",
    "@types/p5": "^1.7.7",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "solid-devtools": "^0.34.4",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-solid": "^2.11.10",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0"
  },
  "dependencies": {
    "katex": "^0.16.27",
    "p5": "^2.2.1",
    "prettier": "3.7.4",
    "solid-js": "^1.9.10",
    "uplot": "^1.6.32"
  }
}
